<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="portal">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/portalDS</jta-data-source>
		<class>com.psicovirtual.procesos.modelo.ejb.entity.procesos.Afiliado</class>
		<class>com.psicovirtual.procesos.modelo.ejb.entity.procesos.CargueArchivo</class>
		<class>com.psicovirtual.procesos.modelo.ejb.entity.procesos.DetalleCargue</class>
		<class>com.psicovirtual.procesos.modelo.ejb.entity.procesos.DetalleCarguePK</class>
		<class>com.psicovirtual.procesos.modelo.ejb.entity.procesos.Usuario</class>
		<properties>
			<property name="eclipselink.target-server" value="SunAS9" />
		</properties>
	</persistence-unit>





</persistence>
